{
  "name": "state-relay",
  "version": "0.1.0",
  "description": "Encrypted state sync and verifiable agent coordination over Nostr",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "nostr-tools": ">=2.0.0"
  },
  "devDependencies": {
    "nostr-tools": "^2.23.0",
    "typescript": "^5.5.0",
    "tsup": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": ["nostr", "state", "sync", "relay", "agent", "coordination", "encrypted"],
  "license": "MIT"
}
